{
  "name": "@6boris/awesome-web3-contracts",
  "description": "",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "6boris",
    "url": "https://github.com/6boris"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "forge",
    "foundry",
    "smart-contracts",
    "solidity",
    "template"
  ],
  "scripts": {
    "clean": "rm -rf cache out",
    "build": "forge build",
    "lint": "bun run lint:sol && bun run prettier:check",
    "lint:sol": "forge fmt --check && bun solhint \"{contracts,foundry/script,foundry/test}/**/*.sol\"",
    "prettier:check": "prettier --check \"**/*.{json,md,yml}\" --ignore-path \".prettierignore\"",
    "prettier:write": "prettier --write \"**/*.{json,md,yml}\" --ignore-path \".prettierignore\"",
    "test": "forge test",
    "test:coverage": "forge coverage",
    "test:coverage:report": "forge coverage --report lcov && genhtml lcov.info --branch-coverage --output-dir coverage"
  },
  "dependencies": {
    "@aave/core-v3": "^1.19.3",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@openzeppelin/contracts": "^5.3.0",
    "@openzeppelin/contracts-upgradeable": "^5.3.0",
    "@openzeppelin/contracts-upgradeable-v4.7.1": "github:OpenZeppelin/openzeppelin-contracts-upgradeable#v4.7.1",
    "@openzeppelin/contracts-upgradeable-v4.7.3": "github:OpenZeppelin/openzeppelin-contracts-upgradeable#v4.7.3",
    "@openzeppelin/contracts-v4.7.1": "github:OpenZeppelin/openzeppelin-contracts#v4.7.1",
    "@openzeppelin/contracts-v4.7.3": "github:OpenZeppelin/openzeppelin-contracts#v4.7.3",
    "@prb/test": "^0.6.4",
    "@scure/bip39": "^1.6.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4",
    "bignumber.js": "^9.3.0",
    "dayjs": "^1.11.13",
    "ethers": "^6.14.3",
    "forge-std": "github:foundry-rs/forge-std#v1.9.6",
    "prettier": "^3.5.3",
    "solady": "^0.1.19",
    "solhint": "^5.0.5",
    "solmate": "^6.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "viem": "^2.30.5",
    "web3": "^4.16.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
